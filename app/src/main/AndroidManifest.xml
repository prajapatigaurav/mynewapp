<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.doorstep.priyagupta.partner">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/lo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashActivity"
            android:configChanges="orientation|screenSize|keyboardHidden">


            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".MainActivity"
            android:configChanges="orientation|screenSize|keyboardHidden" >
        </activity>
        <activity android:name=".ProfileActivity" />
        <activity
            android:name=".ViewActivity"
            android:configChanges="orientation|screenSize|keyboardHidden" >


        </activity>


        <activity
            android:name=".ChangeProfileActivity"
            android:configChanges="orientation|screenSize|keyboardHidden" />
        <activity
            android:name=".AboutmeActivity"
            android:configChanges="orientation|screenSize|keyboardHidden" />
        <activity android:name=".MailActivity" />
        <activity android:name=".AboutViewActivity" />
        <activity android:name=".AfterProfilleActivity"></activity>
    </application>

</manifest>